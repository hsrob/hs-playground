<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>JS Playground - @ViewBag.Title</title>
    <link rel="stylesheet" href="//fonts.googleapis.com/css?family=Open+Sans:300,400,700">
    @Styles.Render("~/Assets/bundle/styles")
    @Styles.Render("~/Assets/bundle/styles/order")
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.10.1/jquery.min.js" type="text/javascript"></script>
    <script src="//ajax.aspnetcdn.com/ajax/jquery.validate/1.11.1/jquery.validate.min.js" type="text/javascript"></script>
    <script src="//ajax.aspnetcdn.com/ajax/jquery.validate/1.11.1/additional-methods.min.js" type="text/javascript"></script>
    @Scripts.Render("~/Assets/bundle/scripts")
</head>
<body>
    <div class="container" id="main-page">
        <div class="navbar navbar-fixed-top">
            <div class="container">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-responsive-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="@Url.Action("Index", "Home")">ToDo Master 9001</a>
                <div class="nav-collapse collapse navbar-responsive-collapse">
                    <ul class="nav navbar-nav">
                        <li><a href="@Url.Action("Index", "Home")">Home</a></li>

                    </ul>
                </div>
            </div>
        </div>
        <div class="alert">
            Because <a target="_blank" href="http://www.youtube.com/watch?v=nbgdMbsUxGI">It's</a>... <a target="_blank" href="http://www.youtube.com/watch?v=LWDfT_ZMbCg">Over</a>... <a target="_blank" href="http://www.youtube.com/watch?feature=player_detailpage&v=LHqEwIadhO8&t=25">9000!!!</a>
        </div>
        <div class="row">
            <div class="col-12">
                @RenderBody()
            </div>
        </div>
    </div>
    @Scripts.Render("~/Assets/bundle/scripts/order")
    @RenderSection("Scripts", false)
</body>
</html>
